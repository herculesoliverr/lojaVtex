{
  "stack-layout#cover": {
    "props": {
      "blockClass": "cover"
    },
    "children": [
      "flex-layout.row#cover-image",
      "flex-layout.row#headline"
    ]
  },
  "flex-layout.row#cover-image": {
    "props": {
      "experimentalHideEmptyCols": true
    },
    "children": [
      "responsive-layout.phone#cover-mobile",
      "responsive-layout.tablet#cover-tablet",
      "responsive-layout.desktop#cover-desktop"
    ]
  },
  "responsive-layout.tablet#cover-tablet": {
    "children": [
      "image#cover-desktop"
    ]
  },
  "responsive-layout.desktop#cover-desktop": {
    "children": [
      "image#cover-desktop"
    ]
  },
  "responsive-layout.phone#cover-mobile": {
    "children": [
      "image#cover-mobile"
    ]
  },
  "flex-layout.row#headline": {
    "props": {
      "horizontalAlign": "center",
      "verticalAlign": "center",
      "blockClass": "headline"
    },
    "children": [
      "flex-layout.col#cover-content"
    ]
  },
  "flex-layout.col#cover-content": {
    "props": {
      "width": "grow",
      "horizontalAlign": "center",
      "blockClass": "headlinecover"
    },
    "children": [
      "rich-text#store-headline"
    ]
  },
  "image#cover-desktop": {
    "props": {
      "height": "480px",
      "blockClass": "coverImage",
      "src": "assets/hero-desktop.jpg"
    }
  },
  "image#cover-mobile": {
    "props": {
      "height": "400px",
      "blockClass": "coverImage",
      "src": "assets/hero-mobile.jpg"
    }
  },
  "rich-text#store-headline": {
    "props": {
      "font": {
        "desktop": "t-heading-1",
        "mobile": "t-heading-3"
      },
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "text": "A sua loja infantil !",
      "blockClass": "storeHeadline"
    }
  }
}